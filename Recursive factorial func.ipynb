    "def fact(n):\n",
    "    if (n==0) or (n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        result=n*fact(n-1)\n",
    "    return result\n",
    "n=int(input(\"Enter the number::\"))\n",
    "fact(n)"
