    "n=int(input())\n",
    "upper=n//2+1\n",
    "lower=n//2\n",
    "for i in range(1,upper+1):\n",
    "    print(\" \"*(upper-i),\"*\"*(2*i-1))\n",
    "for j in range(lower,0,-1):\n",
    "    print(\" \"*(upper-j),\"*\"*(2*j-1))"
