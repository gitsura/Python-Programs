    "import math\n",
    "orig=num\n",
    "num=145\n",
    "sum=0\n",
    "while orig>0:\n",
    "    digit=orig%10\n",
    "    sum=sum+math.factorial(digit)\n",
    "    orig//=10\n",
    "if sum==num:\n",
    "    print(\"strong number\")\n",
    "else:\n",
    "    print(\"not strong number\")\n",
    "    \n"
